<Window x:Class="WorkersCatalog.View.ChooseWorker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ChooseWorker" Height="300" Width="300" ResizeMode="NoResize">
    <StackPanel>
        <ListBox Name="lstChoose" Height="200" Margin="10" SelectionChanged="lstChoose_SelectionChanged"/>
        <StackPanel>
            <Button Content="Choose" Click="OnChangeButtonClick"/>
            <Button Content="Cancel" Click="OnCancelClick" />
        </StackPanel>
    </StackPanel>
</Window>
