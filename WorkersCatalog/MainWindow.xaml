<Window x:Class="WorkersCatalog.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Workers database" Height="260" Width="200" ResizeMode="NoResize" >
    <StackPanel>
        <Button Content="Workers report" Click="OnReportClick" Margin="10 10 10 0" />
        <Button Content="Edit workers" Click="OnEditClick" Margin="10 10 10 0" />
        <Button Content="Database settings" Click="OnDatabaseEditClick" Margin="10 10 10 0" />
        <Button Content="Search" Click="OnSearchClick" Margin="10 10 10 0" />
        <Button Content="LoggerSettings" Click="OnLoggerClick" Margin="10 10 10 0" />

        <Button Content="Notes" Click="OnNotesClick" Margin="10 10 10 0" />
        <Button Content="Exit" Click="OnExitClick" Margin="10 10 10 0" />

    </StackPanel>
</Window>
